{
   "extends": "next/core-web-vitals",
   "plugins": ["unused-imports"],
   "rules": {
      "prefer-const": [
         "error",
         {
            "destructuring": "any",
            "ignoreReadBeforeAssign": false
         }
      ],
      "eqeqeq": [2, "allow-null"],
      "arrow-body-style": ["error", "always"],
      "no-var": "error",
      "no-unused-vars": "error"
   }
}
